  },
  "devDependencies": {
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.11.13",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
